{
    "name": "sevens/router",
    "description": "Simple Php High Speed Web Application Router library with PSR-7 Compliance",
    "type": "library",
    "version": "v1.0.0",
    "license": "MIT",
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Seven\\Router\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "require": {
        "php": ">=7.1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "php-di/php-di": "6.0.11",
        "opis/closure": "3.5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4.2",
        "symfony/http-foundation": "^5.2"
    },
    "authors": [
        {
            "name": "Elisha Temiloluwa a.k.a TemmyScope",
            "email": "temmyscope@protonmail.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "router", "routing engine",
        "fast router", "simple router"
    ]
}
